{
  "generators": {
    "TurbulenceSeed": {
      "method": "IncrementalInt"
    }
  },
  "macros": {
    "Vin": 4,
    "Vrated_m2": 8,
    "Vrated": 10,
    "Vrated_p2": 12,
    "Vout": 25,
    "Vmaint": 10,
    "Vref": 50.0,
    "Vref_70pc": 35,
    "Vmean": 10.0,
    "Ve1": 56,
    "Ve50": 70,
    "WindRangeOperating": "#range($Vin, $Vout, 2)",
    "WindRangeParked": "#range(2, $Vref_70pc, 2)",
    "YawRange": [-10.0, 0.0, 10.0],
    "YawRange360": "#range(0, 345, 15)",
    "FinePitch": 0.0,
    "FeatherPitch": 90.0
  },
  "spec": {
    "output_start_time": 30,
    "wind_shear": 0.14,
    "fatigue": {
      "policy:path": "fatigue",
      "dlc1.2": {
        "policy:path": "dlc1.2/WS_{wind_speed}",
        "simulation_time": 600,
        "wind_speed": "$WindRangeOperating",
        "turbulence_intensity": "#NTM(wind_speed)",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 6)",
        "yaw_angle": "$YawRange"
      },
      "dlc2.4": {
        "policy:path": "dlc2.4/WS_{wind_speed}",
        "simulation_time": 600,
        "wind_speed": "$WindRangeOperating",
        "yaw_angle": [-20, 20],
        "turbulence_intensity": "#NTM(wind_speed)",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 3)"
      },
      "dlc3.1": {
        "policy:path": "dlc3.1/WS_{wind_speed}",
        "operation_mode": "idling",
        "transient_mode": "startup",
        "simulation_time": 100,
        "wind_speed": "$WindRangeOperating",
        "wind_type": "NWP",
        "yaw_angle": "$YawRange"
      },
      "dlc4.1": {
        "policy:path": "dlc4.1/WS_{wind_speed}",
        "operation_mode": "normal",
        "transient_mode": "shutdown",
        "simulation_time": 100,
        "wind_speed": "$WindRangeOperating",
        "wind_type": "NWP",
        "yaw_angle": "$YawRange"
      },
      "dlc6.4": {
        "policy:path": "dlc6.4/WS_{wind_speed}",
        "operation_mode": "parked",
        "wind_speed": "$WindRangeParked",
        "turbulence_intensity": "#NTM(wind_speed)",
        "yaw_angle": "$YawRange"
      }
    }
  },
  "extreme": {
    "policy:path": "extreme",
    "dlc1": {
      "operation_mode": "normal",
      "_safety_factor": "$Normal",
      "a": {
        "simulation_time": 600,
        "wind_speed": "$WindRangeOperating",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 6)",
        "yaw_angle": "$YawRange",
        "dlc1.1": {
          "policy:path": "dlc1.1/WS_{wind_speed}",
          "turbulence_intensity": "#NTM(wind_speed)"
        },
        "dlc1.3": {
          "policy:path": "dlc1.3/WS_{wind_speed}",
          "turbulence_intensity": "#ETM(wind_speed)"
        }
      },
      "dlc1.4": {
        "policy:path": "dlc1.4/WS_{wind_speed}",
        "simulation_time": 100,
        "wind_speed": "$WindRangeOperating",
        "wind_gust_type": "ECD"
      },
      "dlc1.5": {
        "policy:path": "dlc1.5/WS_{wind_speed}",
        "simulation_time": 100,
        "wind_speed": "$WindRangeOperating",
        "wind_gust_type": "EWS"
      }
    },
    "dlc2": {
      "operation_mode": "normal",
      "simulation_time": 100,
      "dlc2.1": {
        "policy:path": "dlc2.1/WS_{wind_speed}",
        "wind_speed": "$WindRangeOperating",
        "yaw_angle": "$YawRange",
        "grid_loss_time": 10.0,
        "turbulence_seed": "#repeat(@TurbulenceSeed, 4)",
        "_safety_factor": "$Normal"
      },
      "dlc2.2p": {
        "policy:path": "dlc2.2/runaway/WS_{wind_speed}",
        "simulation_time": 100,
        "wind_speed": "#range(12, 26, 2)",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 12)",
        "pitch_manoeuvre_time": 10.0,
        "initial_pitch[1]": "$FinePitch",
        "final_pitch[1]": "$FeatherPitch",
        "_safety_factor": "$Abnormal"
      },
      "dlc2.2y": {
        "policy:path": "dlc2.2/yaw/WS_{wind_speed}",
        "simulation_time": 600,
        "wind_speed": "$WindRangeOperating",
        "turbulence_seed": "@TurbulenceSeed",
        "yaw_angle": "#range(15, 345, 15)",
        "_safety_factor": "$Abnormal"
      },
      "dlc2.2b": {
        "policy:path": "dlc2.2/stuck/WS_{wind_speed}",
        "simulation_time": 600,
        "wind_speed": "$WindRangeOperating",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 12)",
        "initial_pitch[1]": "$FinePitch",
        "_safety_factor": "$Abnormal"
      },
      "dlc2.3": {
        "policy:path": "dlc2.3/WS_{wind_speed}",
        "simulation_time": 100,
        "wind_speed": ["$Vrated_m2", "$Vrated_p2", "$Vout"],
        "wind_type": "EOG",
        "grid_loss_time": [10, 12, 14, 16],
        "_safety_factor": "$Abnormal"
      }
    },
    "dlc3": {
      "simulation_time": 100,
      "_safety_factor": "$Normal",
      "dlc3.2": {
        "policy:path": "dlc3.2/WS_{wind_speed}",
        "wind_speed": ["$Vin", "$Vrated_m2", "$Vrated_p2", "$Vout"],
        "wind_type": "EOG",
        "gust_start_time": [10, 20, 30, 40]
      },
      "dlc3.3": {
        "policy:path": "dlc3.3/WS_{wind_speed}",
        "wind_speed": ["$Vin", "$Vrated_m2", "$Vrated_p2", "$Vout"],
        "gust_type": "EDC",
        "gust_start_time": [10, 20, 30, 40]
      }
    },
    "dlc4.2": {
      "policy:path": "dlc4.2/WS_{wind_speed}",
      "simulation_time": 100,
      "transient_mode": "shutdown",
      "wind_speed": ["$Vrated_m2", "$Vrated_p2", "$Vout"],
      "wind_type": "EOG",
      "gust_start_time": 10,
      "pitch_manoeuvre_time": [10, 20, 30, 40, 50, 60],
      "_safety_factor": "$Normal"
    },
    "dlc5.1": {
      "policy:path": "dlc5.1/WS_{wind_speed}",
      "simulation_time": 100,
      "wind_speed": ["$Vrated_m2", "$Vrated_p2", "$Vout"],
      "turbulence_intensity": "#NTM(wind_speed)",
      "turbulence_seed": "#repeat(@TurbulenceSeed, 12)",
      "_safety_factor": "$Normal"
    },
    "dlc6": {
      "simulation_time": 600,
      "operation_mode": "idling",
      "_safety_factor": "$Normal",
      "turbulence_intensity": 0.11,
      "wind_shear": 0.11,
      "dlc6.1": {
        "policy:path": "dlc6.1/WS_{wind_speed}",
        "wind_speed": "$Ve50",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 6)",
        "yaw_angle": [-8, 8],
        "_safety_factor": "$Normal"
      },
      "dlc6.2": {
        "policy:path": "dlc6.2/WS_{wind_speed}",
        "wind_speed": "$Ve50",
        "turbulence_seed": "@TurbulenceSeed",
        "yaw_angle": "#range(0, 345, 15)",
        "_safety_factor": "$Abnormal"
      },
      "dlc6.3": {
        "policy:path": "dlc6.3/WS_{wind_speed}",
        "wind_speed": "$Ve1",
        "turbulence_seed": "#repeat(@TurbulenceSeed, 6)",
        "yaw_angle": [-20, 20],
        "_safety_factor": "$Normal"
      }
    },
    "dlc7.1": {
      "policy:path": "dlc7.1/WS_{wind_speed}",
      "simulation_time": 600,
      "operation_mode": "parked",
      "wind_speed": "$Ve1",
      "turbulence_intensity": 0.11,
      "turbulence_seed": "@TurbulenceSeed",
      "initial_azimuth_angle": [0, 30, 90],
      "yaw_angle": "$YawRange360",
      "_saefty_factor": "$Abnormal"
    },
    "dlc8.1": {
      "policy:path": "dlc8.1/WS_{wind_speed}",
      "simulation_time": 600,
      "operation_mode": "parked",
      "wind_speed": "$Vmaint",
      "turbulence_intensity": "#NTM(wind_speed)",
      "turbulence_seed": "#repeat(@TurbulenceSeed, 6)",
      "initial_azimuth_angle": 0,
      "yaw_angle": [-8, 8],
      "_saefty_factor": "$Normal"
    }
  }
}